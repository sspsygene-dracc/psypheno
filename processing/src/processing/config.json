{
  "basedir": "/Users/jbirgmei/prog/sspsygene",
  "out_db": "data/db/sspsygene.db",
  "gene_map_files": {
    "mgi": "data/homology/MGI_EntrezGene.rpt",
    "zfin": "data/homology/human_orthos.txt",
    "hgnc": "data/homology/hgnc_complete_set.txt"
  },
  "tables": [
    {
      "table": "mouse_perturb_deg",
      "in_path": "data/datasets/mouse-perturb-4tf/deg.txt",
      "split_column_map": [
        {
          "source_col": "perturbation",
          "new_col1": "_perturbation_gene",
          "new_col2": "_perturbation_gene_idx",
          "sep": "_"
        }
      ],
      "index_fields": [],
      "entrez_conversions": [
        {
          "column_name": "Gene",
          "out_column_name": "gene_entrez_id",
          "species": "mouse"
        },
        {
          "column_name": "_perturbation_gene",
          "out_column_name": "_perturbation_gene_entrez_id",
          "species": "mouse",
          "ignore_missing": ["NonTarget1", "SafeTarget"]
        }
      ]
    },
    {
      "table": "mouse_perturb_clusterprop",
      "in_path": "data/datasets/mouse-perturb-4tf/clusterprop.txt",
      "split_column_map": [
        {
          "source_col": "guide",
          "new_col1": "_guide_gene",
          "new_col2": "_guide_gene_idx",
          "sep": "_"
        }
      ],
      "index_fields": [],
      "entrez_conversions": []
    },
    {
      "table": "perturb_fish_astro",
      "in_path": "data/datasets/perturb-fish/perturbfish-astro.csv",
      "split_column_map": [],
      "index_fields": [],
      "entrez_conversions": [
        {
          "column_name": "perturbGene",
          "out_column_name": "perturbGene_entrez_id",
          "comment": "this zebrafish dataset seems to list human gene names",
          "species": "human",
          "ignore_missing": ["Control_ST"]
        },
        {
          "column_name": "gene",
          "out_column_name": "gene_entrez_id",
          "comment": "this zebrafish dataset seems to list human gene names",
          "species": "human"
        }
      ]
    },
    {
      "table": "zebra_asd",
      "in_path": "data/datasets/zebraAsd/1-s2.0-S2211124723002541-mmc5.txt",
      "split_column_map": [
        {
          "source_col": "Mutant_Experiment_Sample",
          "new_col1": "_Mutant_Experiment_Sample_gene",
          "new_col2": "_Mutant_Experiment_Sample_gene_idx",
          "sep": "_"
        }
      ],
      "index_fields": [],
      "entrez_conversions": [
        {
          "column_name": "_Mutant_Experiment_Sample_gene",
          "out_column_name": "_Mutant_Experiment_Sample_gene_entrez_id",
          "species": "human",
          "to_upper": true,
          "replace": {
            "SCN1LAB": "SCN1A"
          }
        }
      ]
    }
  ]
}
